<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="INFO">
    <Properties>
        <Property name="filename_OrderCreation">OrderCreation.log</Property>
        <Property name="filename_Scheduler">Scheduler.log</Property>
        <Property name="filename_DashBoard">DashBoard.log</Property>
        <Property name="filename_ExternalBilling">ExternalBilling.log</Property>
        <Property name="filename_Middleware">Middleware.log</Property>
        <Property name="filename_Siga">Siga.log</Property>
        <Property name="filename_Patient">Patient.log</Property>
        <Property name="filename_Results">Results.log</Property>
        <Property name="filename_Stadistics">Stadistics.log</Property>
        <Property name="filename_ChatWs">ChatWs.log</Property>
        <Property name="filename_PrintWs">PrintWs.log</Property>
        <Property name="filename_Events">Events.log</Property>
        <Property name="filename_IntegrationHis">IntegrationHis.log</Property>
        <Property name="filename_Security">Security.log</Property>

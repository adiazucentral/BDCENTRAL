    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
        
        <Routing name ="RoutingOrderCreation">
            <Routes pattern="$${sys:ProyectName}">
                <Route>
                    <RollingFile name="File_OrderCreation" fileName="logs/${sys:ProyectName}/OrderCreation/${filename_OrderCreation}" filePattern="logs/${sys:ProyectName}/OrderCreation/$${date:yyyy-MM}/OrderCreation-%d{MM-dd-yyyy}-%i.log.gz">
                        <PatternLayout>
                            <pattern>[%-5p %d{dd MMM yyyy - HH:mm:ss}] %c{2} - %m%n</pattern>
                        </PatternLayout>
                        <Policies>
                            <TimeBasedTriggeringPolicy />
                            <SizeBasedTriggeringPolicy size="100 MB"/>
                        </Policies>
                        <DefaultRolloverStrategy max="20"/>
                    </RollingFile>
                </Route>
            </Routes>
        </Routing>
        <Routing name ="RoutingScheduler">
